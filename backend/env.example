# OpenRouter AI API Configuration
# Get your API key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Optional: Customize OpenRouter settings
# OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_MODEL=anthropic/claude-3.5-sonnet

# Fallback models (used if primary model fails to produce valid JSON)
OPENROUTER_FALLBACK_MODEL_1=openai/gpt-4o
OPENROUTER_FALLBACK_MODEL_2=anthropic/claude-3-haiku
OPENROUTER_FALLBACK_MODEL_3=meta-llama/llama-3.1-8b-instruct
